{
  "name": "log1x/acf-field-boilerplate",
  "type": "wordpress-plugin",
  "description": "A modernized starting point for custom Advanced Custom Fields field types.",
  "keywords": [
    "acf",
    "acf-field",
    "wordpress",
    "wordpress-plugin"
  ],
  "require": {
    "php": ">=7.2",
    "composer/installers": "~1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "Log1x\\AcfExampleField\\": "src/"
    }
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --exclude=PSR1.Methods.CamelCapsMethodName --extensions=php --standard=PSR12 ."
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
